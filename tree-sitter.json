{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tcl",
      "camelcase": "Tcl",
      "title": "Tcl",
      "scope": "source.tcl",
      "file-types": ["tk", "tcl", "tm"],
      "injection-regex": "^tcl$",
      "highlights": "queries/tcl/highlights.scm",
      "class-name": "TreeSitterTcl"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Tcl grammar for tree-sitter",
    "authors": [
      {
        "name": "Lewis Russell",
        "email": "lewis6991@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter-grammars/tree-sitter-tcl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
